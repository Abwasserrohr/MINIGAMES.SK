import:
  org.bukkit.Bukkit

on join:
  wait 1 second
  set {_plugin} to Bukkit.getServer().getPluginManager().getPlugin("Skript")
  loop all players:
    loop-player.showPlayer({_plugin},player)
    player.showPlayer({_plugin},loop-player)
